{
  "name": "ssws",
  "version": "0.0.2",
  "main": "index.js",
  "types": "dist/index.d.ts",
  "repository": "git@github.com:msfragala/ssws.git",
  "author": "Mitchell Fragala <ms.fragala@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "yarn clear && run-p build:*",
    "build:rollup": "rollup -c",
    "build:ts": "tsc",
    "clear": "rm -rf dist",
    "test": "ava",
    "prerelease": "yarn build",
    "release": "np",
    "git-pre-commit": ".githooks/pre-commit"
  },
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    },
    "./immutable": {
      "import": "./dist/immutable.esm.js",
      "require": "./dist/immutable.js"
    },
    "./reducible": {
      "import": "./dist/reducible.esm.js",
      "require": "./dist/reducible.js"
    },
    "./react": {
      "import": "./dist/react.esm.js",
      "require": "./dist/react.js"
    }
  },
  "files": [
    "dist/**/*",
    "index.js",
    "react.js",
    "immutable.js",
    "reducible.js"
  ],
  "prettier": {
    "arrowParens": "avoid",
    "quoteProps": "consistent",
    "singleQuote": true,
    "semi": true
  },
  "ava": {
    "files": [
      "tests/*.spec.js"
    ],
    "require": [
      "esm"
    ]
  },
  "dependencies": {
    "dlv": "^1.1.3",
    "klona": "^2.0.4"
  },
  "peerDependencies": {
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/react-hooks": "^6.0.0",
    "@types/node": "^15.3.1",
    "@types/react": "^17.0.6",
    "@vercel/git-hooks": "^1.0.0",
    "ava": "^3.15.0",
    "esm": "^3.2.25",
    "global-jsdom": "^8.1.0",
    "jsdom": "^16.5.3",
    "np": "^7.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "rollup": "^2.48.0",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  }
}
