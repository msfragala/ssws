{
  "name": "ssws",
  "version": "0.0.2",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/types/index.d.ts",
  "repository": "git@github.com:msfragala/ssws.git",
  "author": "Mitchell Fragala <ms.fragala@gmail.com>",
  "license": "MIT",
  "scripts": {
    "prebuild": "yarn clear",
    "build": "run-p build:*",
    "build:esbuild": "node esbuild.js",
    "build:ts": "tsc",
    "clear": "rm -rf dist",
    "test": "ava",
    "prerelease": "yarn build",
    "release": "np",
    "git-pre-commit": ".githooks/pre-commit"
  },
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "prettier": {
    "arrowParens": "avoid",
    "quoteProps": "consistent",
    "singleQuote": true,
    "semi": true
  },
  "ava": {
    "files": [
      "tests/*.spec.js"
    ],
    "require": [
      "esm"
    ]
  },
  "dependencies": {
    "dlv": "^1.1.3",
    "klona": "^2.0.4"
  },
  "peerDependencies": {
    "react": ">=16.8"
  },
  "devDependencies": {
    "@testing-library/react": "^11.2.7",
    "@testing-library/react-hooks": "^6.0.0",
    "@types/node": "^15.3.1",
    "@types/react": "^17.0.6",
    "@vercel/git-hooks": "^1.0.0",
    "ava": "^3.15.0",
    "esbuild": "^0.12.1",
    "esm": "^3.2.25",
    "global-jsdom": "^8.1.0",
    "jsdom": "^16.5.3",
    "np": "^7.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "typescript": "^4.2.4"
  }
}
